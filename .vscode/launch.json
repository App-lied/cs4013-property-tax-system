    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-GUI<cs4013-property-tax-system_ffe3e5b8>",
            "request": "launch",
            "vmArgs": "--module-path D:/Java_JFX/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml", 
            "mainClass": "GUI",
            "projectName": "cs4013-property-tax-system_ffe3e5b8"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-HomeGUI<cs4013-property-tax-system_ffe3e5b8>",
